{
  "dashboard": {
    "title": "MLOps Monitoring Dashboard - Nessa ayu safitri",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Total API Requests",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(api_requests_total[5m]))",
            "legendFormat": "Requests/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Duration (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Total Predictions",
        "type": "stat",
        "targets": [
          {
            "expr": "predictions_total"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Model Accuracy",
        "type": "gauge",
        "targets": [
          {
            "expr": "model_accuracy"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8}
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(api_requests_total{status=\"500\"}[5m]))",
            "legendFormat": "Errors/sec"
          }
        ],
        "gridPos": {"h": 4, "w": 12, "x": 12, "y": 8}
      }
    ],
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}
